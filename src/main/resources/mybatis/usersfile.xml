<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="usersfile">

    <insert id="fileInsert">
        INSERT INTO users_file
        (
         up_time
         , u_no
        )
        
        VALUES
       (
       	NOW()
       	, #{uNo}
       )
    </insert>
    
</mapper>